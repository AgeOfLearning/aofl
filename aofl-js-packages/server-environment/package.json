{
  "name": "@aofl/server-environment",
  "version": "4.0.0-alpha.3",
  "description": "Helps determine dev, stage, prod environments based on hostname",
  "main": "index.js",
  "browser": "dist/index.js",
  "module": "index.js",
  "typings": "types.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-ci": "cross-env NODE_ENV=production aofl build --debug",
    "build": "cross-env NODE_ENV=production npx @aofl/cli build",
    "build:dev": "cross-env NODE_ENV=development npx @aofl/cli build",
    "prepublishOnly": "npm run build-ci && npm run docs",
    "docs": "npx jsdoc -c .jsdocrc.json ."
  },
  "keywords": [
    "aofljs",
    "@aofl/server-environment",
    "server environment",
    "server",
    "environment"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:AgeOfLearning/aofl.git"
  },
  "author": "Arian Khosravi <arian.khosravi@aofl.com>",
  "contributors": [
    "Daniel Belisle <daniel.belisle@aofl.com>",
    "Micah Blu <micah.blu@aofl.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@aofl/cli": "^4.0.0-alpha.3",
    "cross-env": "^7.0.3",
    "jsdoc": "^3.6.6",
    "tsd-jsdoc": "^2.5.0"
  }
}

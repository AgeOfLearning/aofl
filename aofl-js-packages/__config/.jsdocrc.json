{
  "source": {
    "includePattern": "index.js$",
    "excludePattern": "(node_modules|documentation|api-docs|webpack)"
  },
  "opts": {
    "template": "../node_modules/@aofl/jsdoc-template",
    "encoding": "utf8",
    "destination": "../docs/v1.x/api-docs/",
    "recurse": true,
    "verbose": true
  },
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "referenceTitle": "Age Of Learning JavaScript SDK",
    "collapse": true,
    "resources": {
      "AofL JS Documentation": "https://ageoflearning.github.io/aofl"
    }
  }
}

{
  "recurseDepth": 5,
  "source": {
    "includePattern": ".*\\.js$",
    "excludePattern": "(node_modules|coverage|__build|examples|__config|\\.sample\\.js|\\.spec\\.js|\\.aofl\\.js|\\.eslintrc\\.js|\\.wct|\\.min\\.js)",
    "include": ["./README.md"]
  },
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "encoding": "utf8",
    "destination": "../docs/v3.x/api-docs/",
    "recurse": true,
    "verbose": true
  },
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "systemName": "Age Of Learning JavaScript SDK",
    "theme": "paper",
    "linenums": true,
    "outputSourceFiles": true,
    "collapseSymbols": false,
    "navType": "vertical",
    "inverseNav": true,
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
